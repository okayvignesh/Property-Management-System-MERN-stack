<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@500&display=swap" rel="stylesheet">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.css" rel="stylesheet" />
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.js"></script>

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
    integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
    integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
    crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
    integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
    crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
    integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
    crossorigin="anonymous"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.js"></script>



  <style>
    body {
      font-family: 'Poppins', sans-serif;
    }

    .navbar {
      background-color: #6b8eab;
    }

    .card {
      background-color: #6b8eab79;
      margin-left: 100px;
      height: 630px;
      margin-top: 10px;
      font-family: Arial, Helvetica, sans-serif;
    }

    .card-body {
      margin-top: -10px;

      font-family: Georgia, 'Times New Roman', Times, serif;
    }

    .bg-img {
      height: 500px;
      background-repeat: no-repeat;
      background-size: cover;
    }


    .btn {
      background-color: #6b8eab;
      color: white;
      width: 300px;
      height: 50px;
      margin-right: 10px;
      font-size: medium;
      font-weight: bold;
      font-family: poppins;
      margin-bottom: 15px;
    }
  </style>
  <title>Home</title>
</head>

<body class="bg-img" style="background-image: url(/image/neighbourhood.jpg);">
  <nav class="navbar navbar-expand-lg navbar-dark">
    <div class="container-fluid">
      <a class="navbar-brand" href="owner_home">
        <img src="/image/abode.png" height="45" alt="" loading="lazy" />
      </a>
      <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#navbarRightAlignExample"
        aria-controls="navbarRightAlignExample" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fas fa-bars"></i>
      </button>
      <div class="collapse navbar-collapse" id="navbarRightAlignExample">
        <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link  px-3" aria-current="page" href="owner_home">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link px-3" aria-current="page" href="owner_add">Add Abodes</a>
          </li>
          <li class="nav-item">
            <a class="nav-link  px-3" aria-current="page" href="owner_tenants">Manage tenants</a>
          </li>
          <li class="nav-item">
            <a class="nav-link  px-3" aria-current="page" href="owner_services">Services</a>
          </li>
          <li class="nav-item">
            <a class="nav-link  px-3" aria-current="page" href="owner_complaints">Complaints</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active px-3" aria-current="page" href="owner_documents">Documents</a>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle px-4" href="#" id="navbarDropdown" role="button"
              data-mdb-toggle="dropdown" aria-expanded="false">
              Account
            </a>
            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
              <li>
                <a class="dropdown-item" href="#">Profile</a>
              </li>
              <li>
                <a class="dropdown-item" href="forgotpassword">Change password</a>
              </li>
              <li>
                <hr class="dropdown-divider" />
              </li>
              <li>
                <a class="dropdown-item" href="owner_login">Logout</a>
              </li>
            </ul>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <br><br>
  <div style=" margin-left:90px">
    <center>
      <h1 style="font-family:Georgia, 'Times New Roman', Times, serif; color:black;">Add Documents</h1>
    </center>
  </div>
  <form method="post" action="/owner_documents">
    <div class="modal fade" id="mymodal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
      aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalCenterTitle">Add Documents</h5>
            <button type="button" class="close" data-MDB-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <div class="row">
              <div class="col">
                <select class="browser-default custom-select" autocomplete="off" id="flatno" name="flatno">
                  <option selected>Select the flat</option>
                  <option value="A1">A1</option>
                  <option value="A2">A2</option>
                  <option value="B1">B1</option>
                  <option value="B2">B2</option>
                  <option value="C1">C1</option>
                  <option value="C2">C2</option>
                  <option value="D1">D1</option>
                  <option value="D2">D2</option>
                </select>
              </div>
            </div><br><br>
            <div class="form-outline">
              <input type="file" class="form-control" style="width: 400px; margin-left: 30px;" id="buildimage"
                name="buildimage" autocomplete="off" />
            </div>
          </div>
          <div class="modal-footer">
            <button type="submit" class="btn btn-primary">ADD</button>
          </div>
        </div>
      </div>
    </div>
  </form>
  <br><br>
  <div class="modal fade" id="mymodalview" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalCenterTitle">View Documents</h5>
          <button type="button" class="close" data-MDB-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="row">
            <div class="col">
              <select class="browser-default custom-select" autocomplete="off" id="flatno" name="flatno">
                <option selected>Select the flat</option>
                <option value="A1">A1</option>
                <option value="A2">A2</option>
                <option value="B1">B1</option>
                <option value="B2">B2</option>
                <option value="C1">C1</option>
                <option value="C2">C2</option>
                <option value="D1">D1</option>
                <option value="D2">D2</option>
              </select>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <a href="/docs/Rental Agreement.pdf"><button type="submit" class="btn btn-primary">View</button></a>
        </div>
      </div>
    </div>
  </div>
  {{#each data}}
  <div class="col">
    <div class="row">
      <div class="card" style="width: 500px;">
        <a href="#"><img class="card-img-top mt-3" src="/image/apartment.jpg" alt=""></a>
        <div class="card-body" style="font-family: 'Poppins', sans-serif;">
          <h5 class="card-title" style="color: white;">Locality : {{locality}}</h5>
          <p class="card-title" style="color: white;">Building ID : {{buildid}} </p>
          <p class="card-text" style="color: white;">Number of flats : {{flats}} </p>
        </div>
        <div class="col">
          <div class="row">
            <button class="btn"
              style="margin-top: 30px; width:200px; background-color: rgba(240, 248, 255, 0.877); color: black; margin-right:50px;"
              data-mdb-toggle="modal" data-mdb-target="#mymodal">Edit</button>
            <button class="btn"
              style="margin-top: 30px; width:200px; background-color:rgba(240, 248, 255, 0.877); color: black"
              data-mdb-toggle="modal" data-mdb-target="#mymodalview">View</button>
          </div>
        </div>
      </div>
      {{/each}}
</body>

</html>