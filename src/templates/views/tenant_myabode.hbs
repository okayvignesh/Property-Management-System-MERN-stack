<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />

    <!-- MDB -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.css" rel="stylesheet" />

    <!-- MDB -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.js"></script>
    <style>
        body {
            font-family: 'Poppins', sans-serif;
        }

        .navbar {
            background-color: #6b8eab;
        }

        .card {
            background-color: rgba(37, 34, 34, 0.651);
            color: white;
            margin-left: 80px;
            margin-top: 100px;
            font-family: Arial, Helvetica, sans-serif;
        }

        .card-body {
            margin-top: -10px;

            font-family: Georgia, 'Times New Roman', Times, serif;
        }

        .bg-img {
            height: 500px;
            background-repeat: no-repeat;
            background-size: cover;
        }

        .btn {
            background-color: white;
            color: #6b8eab;
            width: 300px;
            height: 50px;
            margin-right: 10px;
            font-size: medium;
            font-weight: bold;
            font-family: poppins;
            margin-bottom: 15px;
        }
    </style>
    <title>My abode</title>
</head>

<body class="bg-img" style="background-image: url(/image/neighbourhood.jpg);">
    <nav class="navbar navbar-expand-lg navbar-dark">
        <!-- Container wrapper -->
        <div class="container-fluid">
            <a class="navbar-brand" href="tenant_dashboard">
                <img src="/image/abode.png" height="45" alt="" loading="lazy" />
            </a>
            <!-- Toggle button -->
            <button class="navbar-toggler" type="button" data-mdb-toggle="collapse"
                data-mdb-target="#navbarRightAlignExample" aria-controls="navbarRightAlignExample" aria-expanded="false"
                aria-label="Toggle navigation">
                <i class="fas fa-bars"></i>
            </button>

            <!-- Collapsible wrapper -->
            <div class="collapse navbar-collapse" id="navbarRightAlignExample">
                <!-- Left links -->
                <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link  px-3" aria-current="page" href="tenant_dashboard">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active px-3" aria-current="page" href="tenant_myabode">My Abode</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link px-3" aria-current="page" href="tenant_documents">Documents</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle px-4" href="#" id="navbarDropdown" role="button"
                            data-mdb-toggle="dropdown" aria-expanded="false">
                            Account
                        </a>
                        <!-- Dropdown menu -->
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <li>
                                <a class="dropdown-item" href="#">Profile</a>
                            </li>
                            <li>
                                <a class="dropdown-item" href="forgotpassword">Change password</a>
                            </li>
                            <li>
                                <hr class="dropdown-divider" />
                            </li>
                            <li>
                                <a class="dropdown-item" href="tenant_login">Logout</a>
                            </li>
                        </ul>
                    </li>
                </ul>
                <!-- Left links -->
            </div>
            <!-- Collapsible wrapper -->
        </div>
        <!-- Container wrapper -->
    </nav>

    <body class="bg-img">
        <form method="post" action="/myabode">
            {{#each data}}
            <iframe src={{location}} width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                referrerpolicy="no-referrer-when-downgrade"></iframe>
            <div style="margin-top: 70px; margin-left:90px">
                <h1 style="font-family:Georgia, 'Times New Roman', Times, serif;">{{locality}}</h1>
                <h4 style="font-family:Georgia, 'Times New Roman', Times, serif;">No of flats: {{flats}}</h4>

            </div>
            <div class="card w-75" style="margin-top: 30px;">
                <div class="card-body">
                    <h3 style="font-family:Georgia, 'Times New Roman', Times, serif;">{{buildingno}}</h3>
                    <h3 style="font-family:Georgia, 'Times New Roman', Times, serif;">{{street}}</h3>
                    <h4 style="font-family:Georgia, 'Times New Roman', Times, serif;">{{city}}</h4>
                    <h4 style="font-family:Georgia, 'Times New Roman', Times, serif;">{{state}}</h4>
                </div>
            </div>
        </form>
        {{/each}}
    </body>

</html>